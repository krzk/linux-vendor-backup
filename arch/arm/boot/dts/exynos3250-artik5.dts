/dts-v1/;
#include "exynos3250.dtsi"

/ {
	model = "Samsung ARTIK5 board based on EXYNOS3250";
	compatible = "samsung,artik5", "samsung,exynos3250";

	chosen {
		bootargs = "console=ttySAC2,115200 vmalloc=256M ess_setup=0x46000000";
	};

	memory {
		reg = <0x40000000 0x1F800000>;
	};

	fixed-rate-clocks {
		#address-cells = <1>;
		#size-cells = <0>;

		xrtcxti: clock@0 {
			compatible = "fixed-clock";
			reg = <0>;
			clock-frequency = <32768>;
			#clock-cells = <0>;
			clock-output-names = "xrtcxti";
		};

		xusbxti: clock@1 {
			compatible = "fixed-clock";
			clock-frequency = <24000000>;
			reg = <1>;
			#clock-cells = <0>;
			clock-output-names = "xusbxti";
		};

		xxti: clock@2 {
			compatible = "fixed-clock";
			reg = <2>;
			clock-frequency = <0>;
			#clock-cells = <0>;
			clock-output-names = "xxti";
			};

		xtcxo: clock@3 {
			compatible = "fixed-clock";
			reg = <3>;
			clock-frequency = <26000000>;
			#clock-cells = <0>;
			clock-output-names = "xtcxo";
		};

		audiocdclk: clock@4 {
			compatible = "fixed-clock";
			reg = <4>;
			clock-frequency = <0>;
			#clock-cells = <0>;
			clock-output-names = "audiocdclk";
		};
	};

	i2c@13860000 {
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;

		s2mps14_pmic@66 {
			compatible = "samsung,s2mps14-pmic";
			reg = <0x66>;
			interrupts = <5 0 0>;
			interrupt-parent = <&gpx3>;
			pinctrl-names = "default";
			pinctrl-0 = <&pmic_irq &pm_wrsti>;
			/* RTC: wtsr/smpl */
                        wtsr_en = <1>; /* enable */
                        smpl_en = <1>; /* enable */
                        wtsr_timer_val = <3>;   /* 1000ms */
                        smpl_timer_val = <4>;   /* 500ms */
                        check_jigon = <0>; /* do not check jigon */
                        /* RTC: If it's first boot, reset rtc to 1/1/2014 12:00:00(Wed) */
                        init_time,sec = <0>;
                        init_time,min = <0>;
                        init_time,hour = <12>;
                        init_time,mday = <1>;
                        init_time,mon = <0>;
                        init_time,year = <114>;
                        init_time,wday = <3>;

			regulators {
				buck1_reg: BUCK1 {
					regulator-name = "vdd_mif";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck2_reg: BUCK2 {
					regulator-name = "vdd_arm";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				buck3_reg: BUCK3 {
					regulator-name = "vdd_int";
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <1600000>;
					regulator-always-on;
					regulator-boot-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo13_reg: LDO13 {
					regulator-name = "vdd_zb_2.8";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-initial-mode = <1>;
				};

				ldo16_reg: LDO16 {
					regulator-name = "vdd_lcd_3.3";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-initial-mode = <1>;
				};

				ldo18_reg: LDO18 {
					regulator-name = "vcc_peri_device_2.8";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-initial-mode = <1>;
				};

				ldo19_reg: LDO19 {
					regulator-name = "vcc_avdd_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-initial-mode = <1>;
				};

				ldo20_reg: LDO20 {
					regulator-name = "vcc_peri_device_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-initial-mode = <1>;
				};

				ldo21_reg: LDO21 {
					regulator-name = "zbcore_1.25";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <1250000>;
					regulator-initial-mode = <1>;
				};

				ldo23_reg: LDO23 {
					regulator-name = "vcc_peri_1.8";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-initial-mode = <1>;
				};

				ldo6_reg: LDO6 {
                                        regulator-name = "VDD_USB_AP_1.0V";
                                        regulator-min-microvolt = <1000000>;
                                        regulator-max-microvolt = <1000000>;
                                        regulator-always-on;
					regulator-initial-mode = <1>;
                                };

				ldo8_reg: LDO8 {
                                        regulator-name = "VDD_UOTG_AP_3.3V";
                                        regulator-min-microvolt = <3000000>;
                                        regulator-max-microvolt = <3000000>;
                                };

				ldo2_reg: LDO2 {
					regulator-always-on;
					regulator-initial-mode = <1>;
                                };

				ldo4_reg: LDO4 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };

				ldo5_reg: LDO5 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };

				ldo7_reg: LDO7 {
					regulator-always-on;
                                        regulator-initial-mode = <1>;
                                };
			};
		};
	};

	dwmmc0@12510000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		bypass-smu;
		fixed_volt;
		only_once_tune;
		extra_tuning;
		supports-highspeed;
		enable-cache-control;
		sw_data_timeout;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		samsung,dw-mshc-hs200-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr200-timing = <1 1 2 0>;
		num-ref-clks = <8>;
		gpios = <&gpk0 2 0x1>;
		ciu_clkin = <25 50 50 100 200 100 200 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_bus1 &sd0_bus4 &sd0_bus8>;

		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@12520000 {
		status = "okay";
		num-slots = <1>;
		channel = <1>;
		caps-control;
		bypass-smu;
		clock-gate;
		enable-cclk-on-suspend;
		fixed_volt;
		only_once_tune;
		supports-highspeed;
		broken-cd;
		non-removable;
		enable-sdio-wakeup;
		sw_data_timeout;
		keep-power-in-suspend;
		pm-ignore-notify;
		cd-type-external;
		cd-type = <2>;
		use-broken-voltage;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <222000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr-timing = <3 0 2 0>;
		clk_pin = "gpk1-0";
		clk_addr = "11000000.pinctrl";
		clk_val = <0x3>;
		clk_str_num = <4>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

	dwmmc2@12530000 {
		status = "okay";
		num-slots = <1>;
		bypass-smu;
		supports-highspeed;
		fixed_volt;
		cd-type = <1>;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 4 2>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_cd &sd2_bus1 &sd2_bus4>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

        ion {
                compatible = "samsung,exynos5430-ion";

		contig-names = "mfc_sh",
				"video",
				"mfc_fw",
				"mfc_nfw";

		contig = <2 0x100000 0x0>,
			<11 0xA00000 0x0>,
			<8 0x100000 0x0>,
			<12 0x100000 0x0>;

                ion_noncontig_heap {
                        id-type = <0 0>;
                };

		exynos_contig_heap {
			id-type = <4 6>;
		};

        };

	fimd_fb: fimd_fb {
                /* gpx0[6]- TE external interrupt pin */
                gpios = <&gpx0 4 0xf>;
		lcd-3.0-supply = <&ldo16_reg>;
		lcd-1.8-supply = <&ldo19_reg>;
                fimd: fimd_ctrl {
                        samsung,left_margin = <1>;
                        samsung,right_margin = <1>;
                        samsung,upper_margin = <1>;
                        samsung,lower_margin = <1>;
                        samsung,hsync_len = <1>;
                        samsung,vsync_len = <1>;
                        samsung,xres = <240>;
                        samsung,yres = <240>;
                        samsung,virtual_x = <240>;
                        samsung,virtual_y = <240>;

                        samsung,width = <24>;
                        samsung,height = <24>;
                        samsung,max_bpp = <32>;
                        samsung,default_bpp = <24>;

                        fb_driver_data {
                                fb_win_variant_0 {
                                        has_osd_c = <1>;
                                        has_osd_d = <0>;
                                        has_osd_alpha = <0>;
                                        osd_size_off = <0x08>;
                                        palette_size = <256>;
                                        VALID_BPP_1248 = <1>;
                                        VALID_BPP_13 = <1>;
                                        VALID_BPP_15 = <1>;
                                        VALID_BPP_16 = <1>;
                                        VALID_BPP_18 = <1>;
                                        VALID_BPP_19 = <1>;
                                        VALID_BPP_24 = <1>;
                                        VALID_BPP_25 = <1>;
                                        VALID_BPP_32 = <1>;
                                };
                                fb_win_variant_1 {
                                        has_osd_c = <1>;
                                        has_osd_d = <1>;
                                        has_osd_alpha = <1>;
                                        osd_size_off = <0x0c>;
                                        palette_size = <256>;
                                        VALID_BPP_1248 = <1>;
                                        VALID_BPP_13 = <1>;
                                        VALID_BPP_15 = <1>;
                                        VALID_BPP_16 = <1>;
                                        VALID_BPP_18 = <1>;
                                        VALID_BPP_19 = <1>;
                                        VALID_BPP_24 = <1>;
                                        VALID_BPP_25 = <1>;
                                        VALID_BPP_32 = <1>;
                                };
                                fb_win_variant_2 {
                                        has_osd_c = <1>;
                                        has_osd_d = <1>;
                                        has_osd_alpha = <1>;
                                        osd_size_off = <0x0c>;
                                        palette_size = <256>;
                                        VALID_BPP_1248 = <1>;
                                        VALID_BPP_13 = <1>;
                                        VALID_BPP_15 = <1>;
                                        VALID_BPP_16 = <1>;
                                        VALID_BPP_18 = <1>;
                                        VALID_BPP_19 = <1>;
                                        VALID_BPP_24 = <1>;
                                        VALID_BPP_25 = <1>;
                                        VALID_BPP_32 = <1>;
                                };
                                fb_win_variant_3 {
                                        has_osd_c = <1>;
                                        has_osd_d = <0>;
                                        has_osd_alpha = <1>;
                                        osd_size_off = <0x0>;
                                        palette_size = <256>;
                                        VALID_BPP_1248 = <1>;
                                        VALID_BPP_13 = <1>;
                                        VALID_BPP_15 = <1>;
                                        VALID_BPP_16 = <1>;
                                        VALID_BPP_18 = <1>;
                                        VALID_BPP_19 = <1>;
                                        VALID_BPP_24 = <1>;
                                        VALID_BPP_25 = <1>;
                                        VALID_BPP_32 = <1>;
                                };
                                fb_win_variant_4 {
                                        has_osd_c = <1>;
                                        has_osd_d = <0>;
                                        has_osd_alpha = <1>;
                                        osd_size_off = <0x0>;
                                        palette_size = <256>;
                                        VALID_BPP_1248 = <1>;
                                        VALID_BPP_13 = <1>;
                                        VALID_BPP_15 = <1>;
                                        VALID_BPP_16 = <1>;
                                        VALID_BPP_18 = <1>;
                                        VALID_BPP_19 = <1>;
                                        VALID_BPP_24 = <1>;
                                        VALID_BPP_25 = <1>;
                                        VALID_BPP_32 = <1>;
                                };
                        };
                };

                mipi_dsi: mipi_dsi {
                        /* gpm0[4]-Reset, gpd0[0]-LCD_BL_EN */
                        gpios = <&gpm0 4 0x1>, <&gpd0 0 0x1>;
                        interrupt-parent = <&gic>;
                        interrupts = <0 83 0>;

                        /* Interface definition: 0 - DSIM_COMMAND,
                                                 1 - DSIM_VIDEO */
                        e_interface = <0>;

                        /* Pixel format : 0 - DSIM_CMD_3BPP,
                                          1 - DSIM_CMD_8BPP,
                                          2 - DSIM_CMD_12BPP,
                                          3 - DSIM_CMD_16BPP,
                                          4 - DSIM_VID_16BPP_565,
                                          5 - DSIM_VID_18BPP_666PACKED,
                                          6 - DSIM_18BPP_666LOOSELYPACKED,
                                          7 - DSIM_24BPP_888 */
                        e_pixel_format = <7>;

                        /* main frame fifo auto flush at VSYNC pulse */
                        auto_flush = <0>;
                        eot_disable = <0>;
                        auto_vertical_cnt = <0>;
                        hse = <0>;
                        hfp = <0>;
                        hbp = <0>;
                        hsa = <0>;

                        /* num of data lane : 0 - DSIM_DATA_LANE_1,
                                              1 - DSIM_DATA_LANE_2,
                                              2 - DSIM_DATA_LANE_3,
                                              3 - DSIM_DATA_LANE_4 */
                        e_no_data_lane = <0>;

                        /* byte clock divider : 0 - DSIM_PLL_OUT_DIV8,
                                                1 - DSIM_EXT_CLK_DIV8,
                                                2 - DSIM_EXT_CLK_BYPASS */
                        e_byte_clk = <0>;

                        /* Burst mode : 0 - DSIM_NON_BURST_SYNC_EVENT,
                                        2 - DSIM_NON_BURST_SYNC_PULSE,
                                        1 - DSIM_BURST
                                        4 - DSIM_NON_VIDEO_MODE */
                        e_burst_mode = <1>;

                        /* PMS
                           - 1. LCD_S6E8AA0 : p - 4, m - 80, s - 2
                           - 2. LCD_S6E3FA0(COMMAND): p - 2, m - 46, s - 1
                           - 3. LCD_S6E3FA0(VIDEO): p - 4, m - 75, s - 1
                           - 4. LCD_S6E3HA0(COMMAND): p - 2, m - 46, s - 1
                           - 5. LCD_S6E63J0X03(CMD):  p - 3, m - 125, s -1 */
                        p = <3>;
                        m = <110>;
                        s = <3>;

                        /* D-PHY PLL stable time spec :min = 200usec ~ max 400usec */
                        pll_stable_time = <0x400>;
                        /* escape clk : 7MHz */
                        esc_clk = <7000000>;
                        /* stop state holding counter after bta change count 0 ~ 0xfff */
                        stop_holding_cnt = <0x0f>;
                        /* bta timeout 0 ~ 0xff */
                        bta_timeout = <0xff>;
                        /* lp rx timeout 0 ~ 0xffff */
                        rx_timeout = <0xffff>;
                };
        };

	sysreg_localout: sysreg_localout@10010000 {
		compatible = "samsung,exynos5-sysreg_localout";
		reg = <0x10010000 0x2000>;
	};

	mdev_0: mdev_output {
		compatible = "samsung,exynos5-mdev";
	};

	usb@12480000 {
		vusb_d-supply = <&ldo6_reg>;
		vusb_a-supply = <&ldo8_reg>;
		status = "okay";
	};

	serial@13800000 {
		status = "okay";
	};

	pinctrl@11000000 {
		cfg_wlanen: cfg-wlanen {
			samsung,pins = "gpx2-3";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <3>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <0>;
		};

		cfg_wlanhostwake: cfg-wlanhostwake {
			samsung,pins = "gpx3-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <0>;
		};
	};

	wlan {
		compatible = "samsung,brcm-wlan";
		gpios = <&gpx2 3 0x1	/* WLAN_EN */
			 &gpx3 2 0xF	/* WLAN_HOST_WAKE */
			>;
		pinctrl-names = "default";
		pinctrl-0=<&cfg_wlanen &cfg_wlanhostwake>;
		status = "okay";
	};
};

&pmic_irq {
	samsung,pins = "gpx3-5";
	samsung,pin-function = <0xf>;
	samsung,pin-pud = <3>;
	samsung,pin-drv = <3>;
};

&pm_wrsti {
	samsung,pins = "gpe1-2";
	samsung,pin-con-pdn = <3>;
};
